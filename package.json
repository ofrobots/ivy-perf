{
  "name": "ivy",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "compile:server": "webpack --config webpack.server.config.js --progress --colors",
    "serve:ssr": "node dist/server",
    "build:ssr": "npm run build:client-and-server-bundles && npm run compile:server",
    "build:client-and-server-bundles": "ng build --prod && ng run ivy:server:production"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "git://github.com/angular/animations-builds.git#master-ivy-aot",
    "@angular/common": "git://github.com/angular/common-builds.git#master-ivy-aot",
    "@angular/compiler": "git://github.com/angular/compiler-builds.git#master-ivy-aot",
    "@angular/core": "git://github.com/angular/core-builds.git#master-ivy-aot",
    "@angular/forms": "git://github.com/angular/forms-builds.git#master-ivy-aot",
    "@angular/http": "git://github.com/angular/http-builds.git",
    "@angular/platform-browser": "git://github.com/angular/platform-browser-builds.git#master-ivy-aot",
    "@angular/platform-browser-dynamic": "git://github.com/angular/platform-browser-dynamic-builds.git#master-ivy-aot",
    "@angular/platform-server": "git://github.com/angular/platform-server-builds.git#master-ivy-aot",
    "@angular/router": "^7.2.4",
    "@nguniversal/express-engine": "^7.1.0",
    "@nguniversal/module-map-ngfactory-loader": "v7.0.2",
    "core-js": "^2.6.4",
    "express": "^4.15.2",
    "rxjs": "~6.4.0",
    "tslib": "^1.9.0",
    "zone.js": "^0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.13.1",
    "@angular/cli": "8.0.0-beta.8",
    "@angular/compiler-cli": "git://github.com/angular/compiler-cli-builds.git#master-ivy-aot",
    "@angular/language-service": "^7.2.4",
    "@types/jasmine": "^2.8.16",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "ts-loader": "^5.2.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "^3.2.4",
    "webpack-cli": "^3.2.3"
  }
